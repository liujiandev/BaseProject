<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sky.health">

    <application
        android:name=".base.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sky.health.activity.loading.LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.HTMLActivity" />
        <activity android:name=".activity.user.LoginActivity" />
        <activity android:name=".activity.user.RegisterActivity" />
        <activity android:name=".activity.user.UpdatePwdActivity" />
        <activity android:name=".activity.user.UserInfoActivity" />
        <activity android:name=".activity.main.MainTabActivity"></activity>
    </application>

</manifest>